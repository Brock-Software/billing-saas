  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build --sourcemapClient",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "node ./server/dev-server.js",
    "prisma:studio": "prisma studio",
    "prisma:studio:prod": "./bun-db.sh",
    "format": "prettier --write .",
    "lint": "eslint .",
    "setup": "bun run build && prisma generate && prisma migrate deploy && prisma db seed && playwright install",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "bun run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "bun run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "bunx playwright install --with-deps chromium",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
