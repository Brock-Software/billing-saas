+export async function loader({ request }: LoaderFunctionArgs) {
+	const userId = await requireUserId(request)
+	const user = await prisma.user.findUniqueOrThrow({
+		where: { id: userId },
+		select: {
+			id: true,
+			name: true,
+			email: true,
+			organizations: {
+				select: {
+					id: true,
+					name: true,
