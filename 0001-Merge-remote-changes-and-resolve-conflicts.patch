+
+export async function validateCSRF(formData: FormData, headers: Headers) {
+	try {
+		await csrf.validate(formData, headers)
+	} catch (error) {
+		if (error instanceof CSRFError) {
+			throw new Response('Invalid CSRF token', { status: 403 })
+		}
+		throw error
+	}
+}
