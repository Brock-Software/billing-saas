+				email: z.string().nullish(),
+				stripeKey: z.string().nullish(),
+				autoStop: zfd.checkbox(),
+			}),
+		),
+	}),
+)
+
+export async function action({ request }: ActionFunctionArgs) {
+	const userId = await requireUserId(request)
+	const formData = await request.formData()
+	const { error, data } = await validator.validate(formData)
+	if (error) return validationError(error)
+
+	const user = await prisma.user.update({
+		where: { id: userId },
+		include: { organizations: true },
+		data: {
+			name: data.name,
+			organizations: {
+				deleteMany: {
+					id: { notIn: data.organizations.map(org => org.id).filter(Boolean) },
+				},
+				upsert: data.organizations.map(org => ({
+					where: {
+						id: org.id ?? '',
+					},
+					create: {
+						name: org.name,
+						line1: org.line1,
+						line2: org.line2,
+						city: org.city,
+						state: org.state,
+						zip: org.zip,
+						phone: org.phone,
+						email: org.email,
+						stripeKeyHash: org.stripeKey
+							? encryptData(org.stripeKey, process.env.ENCRYPTION_KEY!)
+							: undefined,
+						autoStop: org.autoStop,
+					},
+					update: {
+						name: org.name,
+						line1: org.line1,
+						line2: org.line2,
+						city: org.city,
+						state: org.state,
+						zip: org.zip,
+						phone: org.phone,
+						email: org.email,
+						stripeKeyHash: org.stripeKey
+							? encryptData(org.stripeKey, process.env.ENCRYPTION_KEY!)
+							: undefined,
+						autoStop: org.autoStop,
+					},
+				})),
+			},
+		},
+	})
+
+	return redirect('/app', {
+		headers: {
+			'Set-Cookie': setOrgId(user.organizations[0].id),
+		},
+	})
+}
+
